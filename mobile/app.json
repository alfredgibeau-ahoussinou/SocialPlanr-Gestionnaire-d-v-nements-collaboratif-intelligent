{
  "expo": {
    "name": "SocialPlanr",
    "slug": "socialplanr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "socialplanr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Organisez vos événements en collaboration avec vos amis",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.socialplanr.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#3B82F6"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.socialplanr.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#3B82F6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee777932-6657-4f48-8db2-efed4bd2d4ef"
      }
    }
  }
}
